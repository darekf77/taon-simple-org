{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Server @simple-org/main",
      "cwd": "${workspaceFolder}/dist/simple-org/main",
      "program": "${workspaceFolder}/dist/simple-org/main/run.js",
      "args": [
        "child=main"
      ],
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register",
        "--experimental-worker"
      ],
      "presentation": {
        "group": "workspaceServers"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Server @simple-org/second",
      "cwd": "${workspaceFolder}/dist/simple-org/second",
      "program": "${workspaceFolder}/dist/simple-org/second/run.js",
      "args": [
        "child=second"
      ],
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register",
        "--experimental-worker"
      ],
      "presentation": {
        "group": "workspaceServers"
      }
    }
  ]
}