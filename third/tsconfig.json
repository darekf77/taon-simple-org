{
  "extends": "./tsconfig.isomorphic.json",
  "compilerOptions": {
    "strictNullChecks": false,
    "rootDir": "../",
    "paths": {
      "@taon-simple-org /main": [
        "../main\\src\\lib"
      ],
      "@taon-simple-org/main/*": [
        "../main\\src\\lib/*"
      ],
      "@taon-simple-org /second": [
        "../second\\src\\lib"
      ],
      "@taon-simple-org/second/*": [
        "../second\\src\\lib/*"
      ],
      "@taon-simple-org /shared": [
        "../shared\\src\\lib"
      ],
      "@taon-simple-org/shared/*": [
        "../shared\\src\\lib/*"
      ],
      "@taon-simple-org /third": [
        "../third\\src\\lib"
      ],
      "@taon-simple-org/third/*": [
        "../third\\src\\lib/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/assets"
  ]
}