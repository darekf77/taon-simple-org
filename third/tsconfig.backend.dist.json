{
  "extends": "./tsconfig.isomorphic.json",
  "compilerOptions": {
    "rootDir": "./tmp-source-dist",
    "paths": {
      "@taon-simple-org /main": [
        "../main\\src\\lib"
      ],
      "@taon-simple-org/main/*": [
        "../main\\src\\lib/*"
      ],
      "@taon-simple-org /second": [
        "../second\\src\\lib"
      ],
      "@taon-simple-org/second/*": [
        "../second\\src\\lib/*"
      ],
      "@taon-simple-org /shared": [
        "../shared\\src\\lib"
      ],
      "@taon-simple-org/shared/*": [
        "../shared\\src\\lib/*"
      ],
      "@taon-simple-org /third": [
        "../third\\src\\lib"
      ],
      "@taon-simple-org/third/*": [
        "../third\\src\\lib/*"
      ]
    }
  },
  "include": [
    "tmp-source-dist/**/*"
  ],
  "exclude": [
    "tmp-source-dist/assets"
  ]
}