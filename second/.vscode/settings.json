{
  "files.exclude": {},
  "workbench.colorCustomizations": {
    "activityBar.background": "#0084ff",
    "activityBar.foreground": "#000000"
  },
  "editor.rulers": [
    100,
    120
  ],
  "html.format.wrapAttributes": "force-aligned",
  "workbench.colorTheme": "Default Light+",
  "tslint.autoFixOnSave": true,
  "eslint.enable": false,
  "tslint.enable": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "search.exclude": {
    "bin": true,
    "local_release": true,
    "node_modules": true,
    ".build": true,
    ".vscode": true,
    "browser": true,
    "dist": true,
    "package-lock.json": true,
    ".firedev": true,
    ".taon": true,
    ".tnp": true,
    "**/bin": true,
    "**/local_release": true,
    "**/node_modules": true,
    "**/.build": true,
    "**/.vscode": true,
    "**/browser": true,
    "**/dist": true,
    "**/package-lock.json": true,
    "**/.firedev": true,
    "**/.taon": true,
    "**/.tnp": true,
    "projects": true
  },
  "window.title": "taon-dev/taon-simple-org/second (@taon-simple-org/second) [ ${activeEditorShort} ]",
  "search.useIgnoreFiles": false,
  "search.include": [
    "**/src/**"
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "/docs-config.jsonc"
      ],
      "url": "./docs-config.schema.json"
    }
  ]
}